<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core" >
	<Dialog
		id="createDialog"
		title="Confirmation">
		<content>
			<Text text="Are you sure you want to create a new Purchase Order" class="sapUiSmallMargin"></Text>
		</content>
		<beginButton>
			<Button text="Confirm" press="onPressConfirm"></Button>
		</beginButton>
		<endButton>
			<Button text="Cancel" press="onPressCancel"></Button>
		</endButton>		
	</Dialog>
</core:FragmentDefinition>




